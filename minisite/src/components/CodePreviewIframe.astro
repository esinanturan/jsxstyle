---
import { Box } from "@jsxstyle/astro";
import type { CSSProperties } from "@jsxstyle/react";

interface Props extends CSSProperties {}
---

<Box
  flex="1 1 auto"
  border="none"
  width="100%"
  height="100%"
  {...Astro.props}
  component="iframe"
  props={{
    title: "Code Preview",
    src: "/code-preview",
    id: "code-preview",
  }}
/>
