{
  "name": "jsxstyle",
  "version": "2.5.1",
  "description": "Inline style system for React and Preact",
  "author": "Pete Hunt",
  "license": "Apache-2.0",
  "repository": "git@github.com:jsxstyle/jsxstyle.git",
  "main": "./lib/react/index.cjs.js",
  "module": "./lib/react/index.es.js",
  "types": "./react/lib/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "lib",
    "**/lib/**/*",
    "!**/lib/**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "npm run build-types && npm run build-js && npm run format-built-files",
    "build-js": "rm -rf lib && rollup -c",
    "build-types": "tsc -b --emitDeclarationOnly",
    "format-built-files": "prettier --write \"**/lib/**/*.{js,ts}\""
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/generator": "^7.17.9",
    "@babel/parser": "^7.17.9",
    "@babel/traverse": "^7.17.9",
    "@babel/types": "^7.17.0",
    "csstype": "^3.0.11",
    "invariant": "^2.2.4",
    "memfs": "^3.4.1"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.17.1",
    "@types/node": "^18.0.0",
    "next": "^13.0.4",
    "rollup": "^3.20.2",
    "solid-js": "^1.6.16",
    "webpack": "^5.72.0"
  },
  "keywords": [
    "jsxstyle",
    "react",
    "preact",
    "css-in-js"
  ],
  "exports": {
    ".": {
      "types": "./react/lib/index.d.ts",
      "import": "./lib/react/index.es.js",
      "require": "./lib/react/index.cjs.js"
    },
    "./lib/base64-loader": {
      "types": "./webpack-plugin/lib/base64Loader.d.ts",
      "import": "./lib/base64-loader/index.es.js",
      "require": "./lib/base64-loader/index.cjs.js"
    },
    "./lib/extract-styles": {
      "types": "./webpack-plugin/lib/utils/ast/extractStyles.d.ts",
      "import": "./lib/extract-styles/index.es.js",
      "require": "./lib/extract-styles/index.cjs.js"
    },
    "./lib/nextjs-plugin": {
      "types": "./webpack-plugin/lib/nextjs.d.ts",
      "import": "./lib/nextjs-plugin/index.es.js",
      "require": "./lib/nextjs-plugin/index.cjs.js"
    },
    "./lib/noop": {
      "types": "./webpack-plugin/lib/noop.d.ts",
      "import": "./lib/noop/index.es.js",
      "require": "./lib/noop/index.cjs.js"
    },
    "./lib/webpack-loader": {
      "types": "./webpack-plugin/lib/loader.d.ts",
      "import": "./lib/webpack-loader/index.es.js",
      "require": "./lib/webpack-loader/index.cjs.js"
    },
    "./solid": {
      "types": "./solid/lib/index.d.ts",
      "import": "./lib/solid/index.es.js",
      "require": "./lib/solid/index.cjs.js"
    },
    "./utils": {
      "types": "./utils/lib/index.d.ts",
      "import": "./lib/utils/index.es.js",
      "require": "./lib/utils/index.cjs.js"
    },
    "./webpack-plugin": {
      "types": "./webpack-plugin/lib/plugin.d.ts",
      "import": "./lib/webpack-plugin/index.es.js",
      "require": "./lib/webpack-plugin/index.cjs.js"
    }
  }
}
