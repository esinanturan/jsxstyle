---
import type { AstroJsxstyleComponentProps } from "../types";
import { cacheSingleton } from "@jsxstyle/core";

type Props<K extends keyof astroHTML.JSX.DefinedIntrinsicElements = "div"> =
  AstroJsxstyleComponentProps<K>;

const { component: Element = "div", slot, ...astroProps } = Astro.props;
const props = cacheSingleton.getComponentProps(astroProps, "class");
---

<Element {...props}><slot /></Element>
